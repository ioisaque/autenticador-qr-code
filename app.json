{
  "expo": {
    "name": "Autenticador QR",
    "slug": "qrcode",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0076F3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1",
      "supportsTablet": false,
      "bundleIdentifier": "com.isaquecosta.qrreader"
    },
    "android": {
      "versionCode": 1,
      "permissions": ["CAMERA"],
      "package": "com.isaquecosta.qrreader"
    }
  }
}